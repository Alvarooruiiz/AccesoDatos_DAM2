<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.acdat.XML.VuelosEntity">
        <table name="vuelos" schema="public" catalog="HibernatePrueba"/>
        <attributes>
            <id name="vueloId">
                <column name="vuelo_id" precision="-1"/>
            </id>
            <basic name="costo">
                <column name="costo" nullable="false" precision="-1"/>
            </basic>
            <basic name="destino">
                <column name="destino" nullable="false" length="50"/>
            </basic>
            <basic name="fechaLlegada">
                <column name="fecha_llegada" nullable="false" length="10"/>
            </basic>
            <basic name="fechaSalida">
                <column name="fecha_salida" nullable="false" length="10"/>
            </basic>
            <basic name="origen">
                <column name="origen" nullable="false" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
